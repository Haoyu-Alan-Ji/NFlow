    "class Point:\n",
    "    def __init__(self, x, y):\n",
    "        self.x = x      # 属性（数据）\n",
    "        self.y = y\n",
    "\n",
    "    def move(self, dx, dy):  # 方法（操作）\n",
    "        self.x += dx\n",
    "        self.y += dy\n",
    "\n",
    "p = Point(1, 2)     # 创建对象（实例化）\n",
    "print(p.x, p.y)     # 1 2\n",
    "p.move(3, -1)\n",
    "print(p.x, p.y)     # 4 1\n"
   ]
  }
